<article class="card">
    <header class="card__header">
        <h1><%= product.title %></h1>
    </header>
    <div class="card__body">
        <img class="card__photo" src="media/notebook.png" alt="Product Image">
        <span><%= product.price %> </span>
        <p><%= product.description %> </p>
        <button>Add to Cart</button>
    </div>
</article>